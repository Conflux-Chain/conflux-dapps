{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Dapps.js",
    "extends": "./base.json",
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@hooks/*": ["src/hooks/*"],
            "@assets/*": ["src/assets/*"],
            "@pages/*": ["src/pages/*"],
            "@utils/*": ["src/utils/*"],
            "@components/*": ["src/components/*"],
            "@router/*": ["src/router/*"],
            "@store/*": ["src/store/*"],
            "@modules/*": ["src/modules/*"]

        }
    }
}
